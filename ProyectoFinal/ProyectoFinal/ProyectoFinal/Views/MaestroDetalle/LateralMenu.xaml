<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoFinal.Views.MaestroDetalle.LateralMenu"
            NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:ProyectoFinal.Views.MaestroDetalle"
             xmlns:pages2="clr-namespace:ProyectoFinal.Views">
  <FlyoutPage.Flyout>
    <pages:LateralMenuFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
        <x:Arguments>
            <!--Set Home view as default-->
            <pages2:Home/>
         </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>